{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,jpeg,gif,svg,webp}",
      "/assets/*.{css,js,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot,webp}",
      "/css/*",
      "/js/*",
      "/api/*",
      "/robots.txt",
      "/sitemap.xml",
      "/404.html",
      "/staticwebapp.config.json",
      "*.{css,js,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot,webp,json,txt,xml}"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".webp": "image/webp",
    ".txt": "text/plain",
    ".xml": "application/xml"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}