{
  "routes": [
    {
      "route": "/contact",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/about",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/solutions/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/automation/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/resources/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/portfolio",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/blogs/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/privacy",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/terms",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/cookies",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/showcase",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,jpeg,gif,svg,webp}",
      "/assets/*.{css,js,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot,webp}",
      "/css/*",
      "/js/*",
      "/api/*",
      "/robots.txt",
      "/sitemap.xml",
      "/404.html",
      "/staticwebapp.config.json",
      "*.{css,js,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot,webp,json,txt,xml}"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".webp": "image/webp",
    ".txt": "text/plain",
    ".xml": "application/xml"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}